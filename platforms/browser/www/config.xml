<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.bomonti2" version="1.4" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Biranı Seç</name>
    <description>
        Teste hemen başla. Puanlarını topla. Kendi biranı keşfet!
    </description>
    <author email="aytac@tersane.com" href="aytac@tersane.com">
        Tersane
    </author>
    <content src="index.html" />
    <access origin="*" />
    <gap:plugin name="cordova-plugin-statusbar" source="npm" spec="~2.1.2" />
    <preference name="orientation" value="landscape" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="StatusBarOverlaysWebView" value="true" />
    <preference name="StatusBarBackgroundColor" value="#c2a671" />
    <preference name="StatusBarStyle" value="lightcontent" />
</widget>
